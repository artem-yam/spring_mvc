<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.epam.jtc.spring.datalayer.dto.AvailableTagsEntity"
           table="AVAILABLE_TAGS" schema="SYSTEM">
        <id name="id">
            <column name="ID" sql-type="number(*)" precision="2147483646"/>
        </id>
        <property name="tag">
            <column name="TAG" sql-type="varchar2(50)" length="50"
                    not-null="true"/>
        </property>
    </class>
</hibernate-mapping>